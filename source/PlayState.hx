	public function moveCamera(isDad:Bool) {
		var pos:Array<Float> = getCameraPosition(isDad);
		camFollow.set(pos[0], pos[1]);
		if (camFollowTween != null) {
			camFollowTween.destroy();
			camFollowTween = null;
